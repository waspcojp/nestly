<% content_for :header do %>
  <%= render "nests/header" %>
<% end %>
<% content_for :body do %>
<style>
</style>
<%= render "shared/icon_upload", obj: @space %>
<div class="container-fluid">
  <h3 class="page-title"><%= t('spaces.create_of', name: @nest.title )%></h3>

  <div class="row">
	<div class="col-sm-3">
	  <%= render "nests/info" %>
	  <%= render "nests/meta" %>
	</div>
	<div class="col-sm-9">
	  <%= form_for(@space, html: { class: "form-horizontal", id: "form", multipart: true }) do |form| %>
		<%= hidden_field_tag :nest, params[:nest] %>
		<%= render "space", form: form, obj: @space %>
		<div class="input-group mb-3">
		  <%= link_to t('create'), '#', class: 'btn btn-success', id: 'submit' %>
		</div>
	  <% end %>
	</div>
  </div>
</div>
<% end %>
