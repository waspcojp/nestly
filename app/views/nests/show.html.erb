<% content_for :header do %>
  <%= render "header" %>
<% end %>
<% content_for :body do %>
<%= render "shared/watch_control" %>
<div class="container-fluid">
  <%= render "shared/errors", record: nil %>
  <%= render "title" %>
  <div class="row">
	<div class="col-sm-3">
	  <%= render "admin" %>
	  <%= render "meta" %>
	</div>
	<div class="col-sm-9">
	  <%= render "entry_list" %>
	  <%= render "space_list" %>
	</div>
  </div>
</div>
<% end %>
